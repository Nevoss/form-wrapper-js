  "author": "Nevo Golan <nevos12@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "rollup": "^1.13.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "dependencies": {},
  "peerDependencies": {}
