{
  "name": "form-wrapper-js",
  "version": "1.0.0",
  "description": "JS abstraction for forms",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:prod": "jest --collectCoverage && npm run lint",
    "build": "tsc --module commonjs && rollup -c rollup.config.js",
    "watch": "rollup -cw",
    "codecov": "codecov",
    "prepublishOnly": "npm run build",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "npm run docs:build && gh-pages -m 'Auto deploy [ci skip]' -d docs/.vuepress/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nevoss/form-wrapper-js"
  },
  "bugs": {
    "url": "https://github.com/Nevoss/form-wrapper-js/issues"
  },
  "homepage": "https://github.com/Nevoss/form-wrapper-js",
  "keywords": [
    "forms",
    "form",
    "vue",
    "form-vue",
    "forms-vue",
    "js"
  ],
  "author": "Nevo Golan <nevos12@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.11",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "codecov": "^3.4.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "faker": "^4.1.0",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "vuepress": "^1.0.0-alpha.47"
  },
  "dependencies": {},
  "peerDependencies": {}
}
