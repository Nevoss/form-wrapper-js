  "version": "1.0.0",
  "description": "JS abstraction for forms",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:prod": "jest --collectCoverage && npm run lint",
